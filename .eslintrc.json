{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier":"off",
      "indent":["error",2],
      "class-methods-use-this":"off",
      "semi":"error",
      "quotes":"error"
    }
}
